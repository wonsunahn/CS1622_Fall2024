/* Class objects and instance objects */
program objects;

class Point {
	declarations
	    int x=10;
        int y=20;
	enddeclarations
	method void f1()
	{
		system.println('x=');	
		system.println(x);
        system.println('y=');	
		system.println(y);
        x := x + 1;
        y := y + 2;
		return;
	}
}

class Main
{
    declarations
	    Point p1;
	enddeclarations
    method int main()
    {
        Point.f1();
        Point.f1();
        p1.f1();
        p1.f1();
        return 0;
    }
}

